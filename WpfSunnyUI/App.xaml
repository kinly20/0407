<Application x:Class="WpfSunnyUI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WpfSunnyUI"
             xmlns:WpfSunnyUI="clr-namespace:WpfSunnyUI"
             >
            <!--StartupUri="MainWindow.xaml"-->
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/WpfSunnyUILibrary;component/MainDictionary.xaml" />
                <ResourceDictionary Source="pack://application:,,,/WpfSunnyUILibrary;component/ButtonDictionary.xaml" />
                <ResourceDictionary Source="pack://application:,,,/WpfSunnyUILibrary;component/CheckBoxDictionary.xaml" />
                <ResourceDictionary Source="pack://application:,,,/WpfSunnyUILibrary;component/ComboBoxDictionary.xaml" />

                <ResourceDictionary>
                    <WpfSunnyUI:StartClass x:Key="StartClass" />
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>

    </Application.Resources>


</Application>
