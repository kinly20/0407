<UserControl x:Class="Engine.LedEx"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Engine"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="100">
    <Grid>
        <Border CornerRadius="1" Grid.RowSpan="2" Margin="0" BorderThickness="0.3" BorderBrush="Black" />
        <Border CornerRadius="0" Grid.RowSpan="2" Margin="0.5" BorderThickness="1" BorderBrush="Black"/>
        <Grid x:Name="GridColor" Background="#FFD8ECFA" Margin="0.5" />
    </Grid>
</UserControl>
