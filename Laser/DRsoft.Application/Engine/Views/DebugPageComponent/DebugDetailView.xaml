                                        <Button Grid.Row="4" Grid.Column="1"  Content="台面B顶起" FontFamily="{StaticResource SourceHanSansSc Bold}" Width="120" Style="{StaticResource MaterialDesignPaperDarkButton}" cal:Message.Attach="[Event Click] = [Action ManualLoadInProcessByIndex(10)]"/>
                                        <Button Grid.Row="4" Grid.Column="2"  Content="台面B下降" FontFamily="{StaticResource SourceHanSansSc Bold}" Width="120" Style="{StaticResource MaterialDesignPaperDarkButton}" cal:Message.Attach="[Event Click] = [Action ManualLoadInProcessByIndex(11)]"/>
                                    </Grid>
                                </materialDesign:Card>
                                <materialDesign:Card
