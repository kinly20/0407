<UserControl
    x:Class="Engine.Views.DebugPageComponent.DebugDetailView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:cal="http://www.caliburnproject.org"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Engine"
    xmlns:localBehaviors="clr-namespace:Engine.Behaviors"
    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:transfer="clr-namespace:Engine.Transfer"
    d:DesignWidth="1775"
    mc:Ignorable="d">
    <UserControl.Resources>
        <transfer:ValueConvert x:Key="BindingConvert" />
        <transfer:BoolValueConvert x:Key="BothBoolConvert"/>
        <Style x:Key="TextBoxNumberLimit" TargetType="{x:Type TextBox}">
            <Setter Property="InputMethod.IsInputMethodEnabled" Value="False" />
            <EventSetter Event="PreviewTextInput" Handler="TextBoxPreviewTextInput" />
        </Style>
    </UserControl.Resources>
    <Grid Width="1775">
        <Grid.RowDefinitions>
            <RowDefinition Height="357*"/>
            <RowDefinition Height="352*"/>
        </Grid.RowDefinitions>
        <materialDesign:Card
            Width="1775"
            Margin="2,2,-2,2"
            Background="{StaticResource DrLaserBackGround}" Grid.RowSpan="2">
            <TabControl
                materialDesign:ColorZoneAssist.Mode="Dark"
                materialDesign:NavigationRailAssist.ShowSelectionBackground="True"
                Background="{StaticResource DrLaserBackGround}"
                SnapsToDevicePixels="True"
                Style="{StaticResource MaterialDesignNavigationRailTabControl}"
                TabStripPlacement="Top">
                <TabItem Width="85">
                    <TabItem.Header>
                        <StackPanel Width="auto" Height="auto">
                            <materialDesign:PackIcon
                                Width="40"
                                Height="40"
                                HorizontalAlignment="Center"
                                Kind="Forum" />
                            <TextBlock
                                HorizontalAlignment="Center"
                                FontFamily="{StaticResource SourceHanSansSc Bold}"
                                FontSize="20"
                                Text="手动调试" />
                        </StackPanel>
                    </TabItem.Header>
                    <StackPanel Orientation="Vertical">
                        <materialDesign:Card Margin="2" Background="{StaticResource DrLaserBackGround}">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                <materialDesign:Card IsEnabled="{Binding MachineStatus.ManualOperationComplete}"
                                    Grid.Column="0"
                                    Margin="5"
                                    Background="{StaticResource DrLaserBackGround}">
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                        </Grid.RowDefinitions>
                                        <TextBlock
                                            Grid.Row="0"
                                            Grid.Column="0"
                                            Grid.ColumnSpan="2"
                                            Margin="10,10"
                                            VerticalAlignment="Center"
                                            FontFamily="{StaticResource SourceHanSansSc Bold}"
                                            FontSize="16"
                                            Foreground="{StaticResource DrLaserWhiteFontColor}">
                                            进料控制
                                        </TextBlock>
                                        <ToggleButton
                                            Grid.Row="1"
                                            Grid.Column="1"
                                            Width="40"
                                            Height="40"
                                            Margin="5,0"
                                            IsChecked="{Binding MachineStatus.ManualOperationComplete}"
                                            IsEnabled="False"
                                            Style="{StaticResource LedGreenButtonStyle}" />
                                        <TextBlock
                                            Grid.Row="1"
                                            Grid.Column="2"
                                            VerticalAlignment="Center"
                                            FontFamily="{StaticResource SourceHanSansSc Bold}"
                                            FontSize="14"
                                            Foreground="{StaticResource DrLaserWhiteFontColor}">
                                            手动动作完成信号
                                        </TextBlock>
                                        <Button Grid.Row="2" Grid.Column="0"  Content="台面A打齐" FontFamily="{StaticResource SourceHanSansSc Bold}" Width="120" Style="{StaticResource MaterialDesignPaperDarkButton}" cal:Message.Attach="[Event Click] = [Action ManualLoadInProcessByIndex(1)]"/>
                                        <Button Grid.Row="2" Grid.Column="1"  Content="台面A顶起" FontFamily="{StaticResource SourceHanSansSc Bold}" Width="120" Style="{StaticResource MaterialDesignPaperDarkButton}" cal:Message.Attach="[Event Click] = [Action ManualLoadInProcessByIndex(7)]"/>
                                        <Button Grid.Row="2" Grid.Column="2"  Content="台面A下降" FontFamily="{StaticResource SourceHanSansSc Bold}" Width="120" Style="{StaticResource MaterialDesignPaperDarkButton}" cal:Message.Attach="[Event Click] = [Action ManualLoadInProcessByIndex(8)]"/>
