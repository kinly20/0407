                                            Grid.Row="9"
                                            Grid.Column="0"
                                            Margin="50,0,0,0"
                                            VerticalAlignment="Center"
                                            FontFamily="{StaticResource SourceHanSansSc Bold}"
                                            FontSize="14"
                                            Foreground="{StaticResource DrLaserWhiteFontColor}">
