                                            Grid.Row="6"
                                            Grid.Column="1"
                                            Width="85"
                                            Height="26"
                                            Margin="0,0,0,0"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            Background="White"
                                            FontFamily="{StaticResource SourceHanSansSc}"
                                            FontSize="16"
                                            Foreground="Black"
                                            Text="{Binding MachineStatus.Gantry11ActTorque, StringFormat={}{0:F2}}"
                                            TextAlignment="Center" />
                                        <TextBlock
                                            Grid.Row="7"
                                            Grid.Column="0"
                                            Margin="50,0,0,0"
                                            VerticalAlignment="Center"
                                            FontFamily="{StaticResource SourceHanSansSc Bold}"
                                            FontSize="14"
                                            Foreground="{StaticResource DrLaserWhiteFontColor}">
                                            力矩L2
                                        </TextBlock>
                                        <TextBlock
                                            Grid.Row="7"
                                            Grid.Column="1"
                                            Width="85"
                                            Height="26"
                                            Margin="0,0,0,0"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            Background="White"
                                            FontFamily="{StaticResource SourceHanSansSc}"
                                            FontSize="16"
                                            Foreground="Black"
                                            Text="{Binding MachineStatus.Gantry12ActTorque, StringFormat={}{0:F2}}"
                                            TextAlignment="Center" />

                                        <ToggleButton
                                            Grid.Row="5"
                                            Grid.Column="2"
                                            Width="30"
                                            Height="30"
                                            Margin="5,0"
                                            IsChecked="{Binding AxisGantry1.Moving}"
                                            IsEnabled="False"
                                            Style="{StaticResource LedGreenButtonStyle}" />
                                        <TextBlock
                                            Grid.Row="5"
                                            Grid.Column="3"
                                            VerticalAlignment="Center"
                                            FontFamily="{StaticResource SourceHanSansSc Bold}"
