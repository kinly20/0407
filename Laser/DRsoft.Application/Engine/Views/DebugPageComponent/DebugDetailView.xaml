                                        <Button Grid.Row="3" Grid.Column="1"  Content="进料由A到B" FontFamily="{StaticResource SourceHanSansSc Bold}" Width="120" Style="{StaticResource MaterialDesignPaperDarkButton}" cal:Message.Attach="[Event Click] = [Action ManualLoadInProcessByIndex(4)]"/>
